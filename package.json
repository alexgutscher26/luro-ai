{
  "name": "luro-ai",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:a11y": "jest --testPathPattern=a11y",
    "test:a11y:watch": "jest --testPathPattern=a11y --watch",
    "test:a11y:coverage": "jest --testPathPattern=a11y --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "postinstall": "prisma generate",
    "prepare": "husky install",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:pull": "prisma db pull",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "prisma migrate reset --force",
    "db:deploy": "prisma migrate deploy",
    "db:status": "prisma migrate status",
    "db:format": "prisma format",
    "db:validate": "prisma validate",
    "db:dev": "prisma migrate dev",
    "db:reset:seed": "npm run db:reset && npm run db:seed",
    "db:migrate:status": "tsx scripts/migrate.ts status",
    "db:migrate:create": "tsx scripts/migrate.ts create",
    "db:migrate:deploy": "tsx scripts/migrate.ts deploy",
    "db:migrate:deploy:force": "tsx scripts/migrate.ts deploy --force",
    "db:migrate:reset": "tsx scripts/migrate.ts reset --force",
    "env:validate": "tsx scripts/validate-env.ts",
    "env:check": "npm run env:validate",
    "prebuild": "npm run env:validate",
    "predev": "npm run env:validate",
    "db:migrate:check": "npx prisma migrate diff --from-empty --to-schema-datamodel prisma/schema.prisma",
    "db:backup": "echo 'Backup command depends on your database provider'",
    "db:restore": "echo 'Restore command depends on your database provider'",
    "sonar": "sonar-scanner",
    "sonar:local": "sonar-scanner -Dsonar.host.url=http://localhost:9000",
    "complexity": "tsx scripts/complexity-analysis.ts",
    "complexity:watch": "tsx scripts/complexity-analysis.ts --watch",
    "complexity:ci": "tsx scripts/complexity-analysis.ts --ci",
    "quality:full": "npm run lint && npm run type-check && npm run test:coverage && npm run complexity",
    "analyze": "npm run complexity && npm run sonar",
    "quality:check": "npm run lint && npm run type-check && npm run test:coverage",
    "quality:fix": "npm run format && npm run lint -- --fix",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
